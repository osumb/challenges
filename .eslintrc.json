{
    "rules": {
        "indent": [
            2,
            2
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "no-console": [
          0
        ],
        "curly": [
          "error",
          "multi-line"
        ],
        "react/jsx-indent": [
          2,2
        ],
        "react/jsx-indent-props": "off",
        "react/no-multi-comp": "off"
    },
    "env": {
        "browser": true,
        "es6": true,
        "jquery": true,
        "node": true
    },
    "extends": "@btmills/eslint-config-btmills/es6",
    "extends": "@btmills/eslint-config-btmills/react"
}
