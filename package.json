    "babel-loader": "6.2.7",
    "babel-preset-react-app": "^2.0.0",
    "bcrypt": "^0.8.6",
    "body-parser": "~1.13.2",
    "cancelable-promise": "^2.2.0",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "1.1.3",
    "change-case": "^3.0.0",
    "classnames": "^2.2.5",
    "connect-history-api-fallback": "1.3.0",
    "connect-redis": "^3.0.2",
    "cookie-parser": "~1.3.5",
    "cross-spawn": "4.0.2",
    "css-loader": "0.26.0",
