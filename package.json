    "gzip-size": "3.0.0",
    "html-webpack-plugin": "2.24.0",
    "http-proxy-middleware": "0.17.2",
    "jest": "17.0.2",
    "json-loader": "0.5.4",
    "jsonwebtoken": "^7.2.0",
    "jwt-decode": "^2.1.0",
    "keycode": "^2.1.7",
    "material-ui": "^0.16.4",
    "moment": "^2.12.0",
    "morgan": "^1.7.0",
    "node-cron": "^1.1.2",
    "node-sass": "^3.13.0",
    "node-xlsx": "^0.7.1",
