{
  "name": "challenges",
  "version": "1.0.0",
  "author": "Alex Tareshawty",
  "description": "Challenge signup manager for OSUMB",
  "engines": {
    "node": "6.2.1"
  },
  "private": true,
  "scripts": {
    "start": "node bin/www",
    "build": "node client/scripts/build.js",
    "dev": "foreman start -f Procfile.dev",
    "lint": "eslint .",
    "heroku-postbuild": "npm run build",
    "schema": "script/schema",
    "seed": "./bin/seed",
    "server": "nodemon --ignore client/ bin/www",
    "test": "bin/test"
  },
  "dependencies": {
    "async": "^2.0.0-rc.5",
    "autoprefixer": "6.5.1",
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "6.17.0",
    "babel-eslint": "7.0.0",
