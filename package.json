{
  "name": "challenges",
  "version": "1.0.0",
  "author": "Alex Tareshawty",
  "description": "Challenge signup manager for OSUMB",
  "engines": {
    "node": "6.2.1"
  },
  "private": true,
  "scripts": {
    "dev": "webpack && NODE_ENV=dev nodemon --use_strict ./bin/www",
    "lint": "eslint .",
    "start": "script/server",
    "test": "script/test",
    "seed": "script/seed",
    "migrate:current": "knex --knexfile config/knexfile.js migrate:currentVersion",
    "migrate:latest": "knex --knexfile config/knexfile.js migrate:latest",
    "migrate:make": "node utils/createMigration",
    "migrate:rollback": "knex --knexfile config/knexfile.js migrate:rollback"
  },
  "dependencies": {
    "async": "^2.0.0-rc.5",
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.13.2",
    "bcrypt": "^0.8.6",
    "body-parser": "~1.13.2",
    "connect-redis": "^3.0.2",
    "cookie-parser": "~1.3.5",
    "css-loader": "^0.23.1",
    "debug": "~2.2.0",
    "es6-promise": "^3.2.1",
    "eslint": "^2.11.1",
    "express": "~4.13.1",
    "express-handlebars": "^3.0.0",
    "express-session": "^1.13.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "github-api": "^2.3.0",
    "jquery": "^3.1.0",
    "knex": "^0.11.10",
    "moment": "^2.12.0",
    "morgan": "^1.7.0",
    "node-sass": "^3.7.0",
    "node-schedule": "^1.1.1",
    "nodemon": "^1.10.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "pg": "^4.5.1",
    "pg-pool": "^1.4.3",
    "redis": "^2.6.0-2",
    "sass-loader": "^3.2.0",
    "sendgrid": "^3.0.5",
    "serve-favicon": "~2.3.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "@btmills/eslint-config-btmills": "^1.0.0-beta.6",
    "eslint": "^2.11.1",
    "jasmine": "^2.4.1",
    "node-minify": "^1.3.7",
    "node-xlsx": "^0.7.1",
    "uglify-js": "^2.6.2"
  }
