    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "path-exists": "2.1.0",
    "pg": "^4.5.1",
    "pg-pool": "^1.4.3",
    "postcss-loader": "^1.0.0",
    "promise": "7.1.1",
    "react": "^15.4.1",
    "react-datetime": "^2.8.1",
    "react-dev-utils": "^0.4.0",
    "react-dom": "^15.4.1",
    "react-media": "^1.4.0",
    "react-router-dom": "4.0.0-beta.3",
